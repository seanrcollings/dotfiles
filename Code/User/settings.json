{
  "workbench.colorTheme": "One Monokai",
  "workbench.iconTheme": "material-icon-theme",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.stablePeek": true,
  "workbench.startupEditor": "newUntitledFile",
  "files.autoSave": "onWindowChange",
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.view.pdf.ref.viewer": "tabOrBrowser",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/__pycache__": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.mypy_cache": true,
    "**/.project": true,
    "**/.settings": true,
    "**/**.class": true,
    "**/**.pytest_cache": true,
    "**/**.venv": true
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "files.trimTrailingWhitespace": true,
  "search.exclude": {
    "env/**": true,
    "node_modules": true
  },
  "tabnine.experimentalAutoImports": true,
  "editor.renderWhitespace": "none",
  "editor.fontFamily": "Fira Code, SauceCodePro Nerd Font",
  "launch": {
    "configurations": [],
    "compounds": []
  },
  "python.languageServer": "Pylance",
  "clang-format.executable": "/usr/bin/clang-format",

  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.tabSize": 2
  },
  "[ruby]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "rebornix.ruby"
  },
  "[rust]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "rust-lang.rust"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.tabSize": 2
  },
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "on",
      "strings": "on",
      "other": "on"
    },
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[scss]": {
    "editor.tabSize": 2
  },
  "[erb]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnType": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "[svelte]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "ruby.codeCompletion": "rcodetools",
  "ruby.intellisense": "rubyLocate",
  "ruby.useLanguageServer": true,
  "ruby.linter.run": "onSave",
  "ruby.interpreter.commandPath": "/home/sean/.asdf/shims/ruby",
  "solargraph.bundlerPath": "/home/sean/.asdf/shims/bundle",
  "solargraph.commandPath": "/home/sean/.asdf/shims/solargraph",
  "editor.formatOnSave": true,
  "eslint.format.enable": true,
  "python.showStartPage": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.editor.enablePreview": false,
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace"
  },
  "vim.handleKeys": {
    "<C-w>": false
  },
  "window.titleBarStyle": "custom",
  "window.customMenuBarAltFocus": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "terminal.external.linuxExec": "alacritty",
  "notebook.kernelProviderAssociations": [],
  "sorbet.lspConfigs": [
    {
      "id": "stable",
      "name": "Sorbet",
      "description": "Stable Sorbet Ruby IDE features",
      "cwd": "${workspaceFolder}",
      "command": [
        "bundle",
        "exec",
        "srb",
        "typecheck",
        "--lsp",
        "--disable-watchman"
      ]
    }
  ],

  "ruby.rubocop.suppressRubocopWarnings": true,
  "editor.semanticHighlighting.enabled": true,
  "javascript.implicitProjectConfig.checkJs": true,
  "python.formatting.provider": "black",
  "files.associations": {
    "*.jsx": "javascriptreact",
    "*.pl": "prolog"
  },
  "ruby.format": false,
  "ruby.lint": {
    "rubocop": {
      "useBundler": true,
      "configFilePath": ".rubocop.yml",
      "forceExclusion": true
    }
  },
  "explorer.confirmDragAndDrop": false,
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "explorer.confirmDelete": false,
  "java.refactor.renameFromFileExplorer": "autoApply",
  "editor.fontSize": 13,
  "rust-client.rustupPath": "/home/sean/.cargo/bin/rustup",
  "mypy.dmypyExecutable": "~/.asdf/shims/dmypy",
  "security.workspace.trust.untrustedFiles": "open",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.includeLanguages": {
    "html.erb": "html",
    "erb": "html",
    "aspnetcorerazor": "html",
    "postcss": "css"
  },
  "omnisharp.monoPath": "/usr/bin/mono",
  "ruby.useBundler": true,
  "ruby.linter.executablePath": "/home/sean/.asdf/shims/ruby",
  "editor.fontLigatures": true,
  "python.defaultInterpreterPath": "/home/sean/.asdf/shims/python",
  "redhat.telemetry.enabled": false,
  "python.linting.pylintArgs": ["--rcfile=.pylintrc"],
  "workbench.colorCustomizations": {
    "[Monokai Gravity]": {
      "editor.background": "#252525",
      "editor.foreground": "#dddddd",
      "editor.lineHighlightBackground": "#2d2d2d",
      "editor.selectionBackground": "#333",
      "editorCursor.foreground": "#6eadd5",
      "editorIndentGuide.activeBackground": "#6b8595",
      "editorWhitespace.foreground": "#3e3e3e",
      "peekViewEditor.matchHighlightBackground": "#ffffff00",
      "activityBar.background": "#202020",
      "titleBar.activeBackground": "#2e2e2e",
      "titleBar.inactiveBackground": "#202020",
      "sideBar.background": "#222222"
      // "peekViewEditor.background": "#ff0000"
    }
  },
  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "[Monokai Gravity]": {
      "rules": {
        "function": {
          "foreground": "#3BC0F0"
        },
        "function.declaration": {
          "foreground": "#a4c267"
        }
      }
    },
    "[One Monokai]": {
      "rules": {
        "module": "#98C379",
        "function": "#3BC0F0"
      }
    }
  },
  "editor.tokenColorCustomizations": {
    "[Monokai Gravity]": {
      "textMateRules": [
        {
          "scope": ["variable.other.constant"],
          "settings": {
            "foreground": "#BE84FF"
          }
        },
        {
          "scope": [
            "punctuation.definition.block.js",
            "punctuation.definition.binding-pattern.object.js.jsx",
            "meta.jsx.children.js.jsx",
            "meta.jsx.children",
            "meta.arrow.tsx",
            "support.variable.property.js.jsx"
          ],
          "settings": {
            "foreground": "#EEEEEE"
          }
        },
        {
          "scope": [
            "punctuation.section.embedded.begin.tsx",
            "punctuation.section.embedded.end.tsx",
            "meta.embedded.expression.js.jsx",
            "punctuation.section.embedded.begin.js.jsx",
            "punctuation.section.embedded.end.js.jsx",
            "punctuation.section.embedded.begin.js",
            "punctuation.section.embedded.end.js",
            "punctuation.definition.template-expression.end",
            "punctuation.definition.template-expression.begin",
            "meta.embedded.line.ruby",
            "punctuation.section.embedded.end.svelte",
            "punctuation.section.embedded.begin.svelte"
          ],
          "settings": {
            "foreground": "#E13E79"
          }
        },
        {
          "scope": "support.class.component.js.jsx",
          "settings": {
            "foreground": "#66D9EF"
          }
        },
        {
          "scope": "variable.language.this",
          "settings": {
            "foreground": "#dc9138"
          }
        },
        {
          "scope": [
            "punctuation.definition.string.end.ruby",
            "punctuation.definition.string.begin.ruby"
          ],
          "settings": {
            "foreground": "#E5B567"
          }
        },
        {
          "scope": ["support.class", "support.class.ruby"],
          "settings": {
            "foreground": "#66D9EF",
            "fontStyle": "italic"
          }
        },
        {
          "scope": ["support.function"],
          "settings": {
            "foreground": "#66D9EF"
          }
        },
        {
          "scope": [
            "variable.language.self.ruby",
            "variable.parameter.function.language.special.self.python"
          ],
          "settings": {
            "foreground": "#FD971F"
          }
        }
      ]
    }
  },

  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format"
  },
  "cmake.configureOnOpen": true,
  "csharp.semanticHighlighting.enabled": true,
  "omnisharp.useGlobalMono": "always",
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": true,
    "ruby": false,
    "javascriptreact": false,
    "rust": false,
    "javascript": false,
    "typescript": false
  },
  "svelte.enable-ts-plugin": true,
  "python.linting.mypyPath": "/home/sean/.asdf/shims/mypy",
  "svelte.ask-to-enable-ts-plugin": false,
  "editor.unicodeHighlight.allowedCharacters": {
    "‚ùØ": true
  },
  "terminal.explorerKind": "external",
  "terminal.integrated.commandsToSkipShell": ["language-julia.interrupt"],
  "julia.symbolCacheDownload": true,
  "julia.enableTelemetry": false,
  "julia.executablePath": "/home/sean/.asdf/shims/julia",
  "window.menuBarVisibility": "compact",
  "editor.bracketPairColorization.enabled": false,
  "python.formatting.blackPath": "/home/sean/.asdf/shims/black",
  "python.analysis.autoImportCompletions": true,
  "python.analysis.autoImportUserSymbols": true,
  "mypy.runUsingActiveInterpreter": true,
  "files.insertFinalNewline": true,
  "svelte.plugin.svelte.note-new-transformation": false,
  "editor.linkedEditing": true,
  "editor.lineNumbers": "relative",
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "svelte.language-server.runtime": "/home/sean/.asdf/shims/node",
  "git.openRepositoryInParentFolders": "never",
  "gitlens.mode.active": "zen",
  "isort.args": ["--wrap-length", "80"]
}