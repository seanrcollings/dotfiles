{
  "workbench.colorTheme": "Orion",
  "workbench.iconTheme": "material-icon-theme",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "editor.stablePeek": true,
  "workbench.startupEditor": "newUntitledFile",
  "files.autoSave": "onWindowChange",
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.view.pdf.ref.viewer": "tabOrBrowser",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/__pycache__": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.mypy_cache": true,
    "**/.project": true,
    "**/.settings": true,
    "**/**.class": true,
    "**/**.pytest_cache": true,
    "**/**.venv": true
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "files.trimTrailingWhitespace": true,
  "search.exclude": {
    "env/**": true,
    "node_modules": true
  },
  "tabnine.experimentalAutoImports": true,
  "editor.renderWhitespace": "none",
  "editor.fontFamily": "Fira Code, SauceCodePro Nerd Font",
  "launch": {
    "configurations": [],
    "compounds": []
  },
  "python.languageServer": "Pylance",
  "clang-format.executable": "/usr/bin/clang-format",

  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.tabSize": 2
  },
  "[ruby]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "rebornix.ruby"
  },
  "[rust]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "rust-lang.rust"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.tabSize": 2
  },
  "[markdown]": {
    "editor.quickSuggestions": true,
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[scss]": {
    "editor.tabSize": 2
  },
  "[erb]": {
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "[svelte]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "ruby.codeCompletion": "rcodetools",
  "ruby.intellisense": "rubyLocate",
  "ruby.useLanguageServer": true,
  "ruby.linter.run": "onSave",
  "ruby.interpreter.commandPath": "/home/sean/.asdf/shims/ruby",
  "solargraph.bundlerPath": "/home/sean/.asdf/shims/bundle",
  "solargraph.commandPath": "/home/sean/.asdf/shims/solargraph",
  "python.linting.pylintEnabled": true,
  "editor.formatOnSave": true,
  "eslint.format.enable": true,
  "python.showStartPage": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.editor.enablePreview": false,
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace"
  },
  "window.titleBarStyle": "custom",
  "window.customMenuBarAltFocus": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "terminal.external.linuxExec": "alacritty",
  "notebook.kernelProviderAssociations": [],

  "ruby.rubocop.suppressRubocopWarnings": true,
  "editor.semanticHighlighting.enabled": true,
  "javascript.implicitProjectConfig.checkJs": true,
  "python.formatting.provider": "black",
  "files.associations": {
    "*.jsx": "javascriptreact",
    "*.pl": "prolog"
  },
  "ruby.format": false,
  "ruby.lint": {
    "rubocop": {
      "useBundler": true,
      "configFilePath": ".rubocop.yml",
      "forceExclusion": true
    }
  },
  "explorer.confirmDragAndDrop": false,
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "explorer.confirmDelete": false,
  "svelte.language-server.runtime": "/usr/bin/node",
  "java.refactor.renameFromFileExplorer": "autoApply",
  "editor.fontSize": 13,
  "rust-client.rustupPath": "/home/sean/.cargo/bin/rustup",
